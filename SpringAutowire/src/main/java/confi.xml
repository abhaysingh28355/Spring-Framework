<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />



	<bean class="com.SpringAutowire.Address" id="id2"
		autowire-candidate="false">
		<property name="hno" value="101" />
		<property name="city" value="Raipur" />
		<property name="state" value="C.G" />
	</bean>

	<bean class="com.SpringAutowire.Address" id="id3">
		<property name="hno" value="102" />
		<property name="city" value="Rewa" />
		<property name="state" value="M.P" />
	</bean>
	<bean class="com.SpringAutowire.Employee" id="id1">
		<property name="eid" value="101" />
		<property name="ename" value="Shivam" />
		<property name="salary" value="5000.50" />



	</bean>
</beans>